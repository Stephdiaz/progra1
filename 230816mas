void impares(int n)		//nuestra los primeros numeros impares
	{int i,imp=1;

for (i=0;i<n;i++)
	{	
		cout<<imp<<"	";
		imp=imp+2;
}
}
void primos(int n)
{
	int i=1,primo=1,j;
	for (j=0;j<n;j++)	
		{while (verdadofalsoprimo(i)==false)
		{i++;}
	cout<<i<<" ";	
	i++;
	}
		

}

bool verdadofalsoprimo(int n)
		{	int i;
		bool k=true;
		for(i=2;i<n;i++){
			if(n%i==0){
				k=false;
			}
		}
		return k;
}

